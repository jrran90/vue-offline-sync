{
  "name": "vue-offline-sync",
  "version": "1.1.0",
  "description": "A Vue 3 component for offline-first syncing. Save data while offline and sync it automatically when back online. Uses IndexedDB for storage.",
  "main": "dist/vue-offline-sync.umd.js",
  "module": "dist/vue-offline-sync.es.js",
  "exports": {
    "import": "./dist/vue-offline-sync.es.js",
    "require": "./dist/vue-offline-sync.umd.js"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrran90/vue-offline-sync"
  },
  "keywords": [
    "vue",
    "vue3",
    "offline",
    "sync",
    "indexedDB",
    "bulk"
  ],
  "author": "jrran90",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrran90/vue-offline-sync/issues"
  },
  "homepage": "https://github.com/jrran90/vue-offline-sync#readme",
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "jsdom": "^26.0.0"
  }
}
